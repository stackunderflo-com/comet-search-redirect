{
  "name": "comet-search-redirect",
  "version": "1.0.0",
  "description": "Chrome extension for Comet browser that redirects single-word searches to Google and keeps multi-word searches on Perplexity AI",
  "keywords": [
    "chrome-extension",
    "comet-browser",
    "search",
    "redirect",
    "perplexity",
    "google"
  ],
  "author": "fpoujol",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "webpack --mode=development --watch",
    "build": "webpack --mode=production",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "package": "npm run build && node scripts/build.js",
    "prepare-store": "node scripts/prepare-store.js",
    "clean": "rimraf dist/ package.zip store-assets.zip"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "archiver": "^5.3.1",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.50.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "html-webpack-plugin": "^5.5.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mini-css-extract-plugin": "^2.7.6",
    "rimraf": "^5.0.5",
    "style-loader": "^3.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "zip-webpack-plugin": "^4.0.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ]
}